{
    "name": "vanilla-cookieconsent",
    "version": "3.1.0-rmdev-2.5",
    "description": "üç™ Simple cross-browser cookie-consent plugin written in vanilla js.",
    "main": "dist/cc.umd.js",
    "module": "dist/cc.esm.js",
    "files": [
        "dist",
        "types"
    ],
    "types": "types/index.d.ts",
    "scripts": {
        "dev": "rollup --config ./rollup-full.config.mjs --watch",
        "build": "pnpm rollup:full & pnpm rollup:core",
        "rollup:full": "rollup --config ./rollup-full.config.mjs",
        "rollup:core": "rollup --config ./rollup-core.config.mjs",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "test": "jest --env=jsdom --runInBand --coverage --silent ./tests"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/orestbida/cookieconsent"
    },
    "keywords": [
        "cookie",
        "consent",
        "gdpr",
        "banner",
        "vanilla"
    ],
    "bugs": {
        "url": "https://github.com/orestbida/cookieconsent/issues"
    },
    "homepage": "https://cookieconsent.orestbida.com",
    "author": "Orest Bida",
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.29.0",
        "@babel/preset-env": "^7.29.0",
        "@rollup/plugin-eslint": "^9.0.5",
        "@rollup/plugin-terser": "^0.4.4",
        "@types/jest": "^29.5.14",
        "babel-jest": "^29.7.0",
        "baseline-browser-mapping": "^1.0.0",
        "cssnano": "^7.0.6",
        "eslint": "^9.39.3",
        "fast-glob": "^3.3.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "postcss-combine-duplicated-selectors": "^10.0.3",
        "rollup": "^4.59.0",
        "rollup-plugin-postcss": "^4.0.2",
        "sass": "^1.97.3",
        "sitemap": "^8.0.2",
        "vitepress": "^1.6.4"
    },
    "jest": {
        "transform": {
            "\\.[jt]sx?$": "babel-jest"
        }
    }
